    "extract-tgz",
    "decompress",
    "decompress-tar",
    "decompress-bz2",
    "decompress-gz",
    "decompress-xz",
    "decompress-tgz",
    ".tar",
    "tar",
    ".tar.bz2",
    "tar.bz2",
    ".tar.gz",
    "tar.gz",
    ".tar.xz",
    "tar.xz",
    ".tgz",
    "tgz",
    "tar-stream",
    "stream"
  ],
  "homepage": "https://github.com/kmalakoff/tar-iterator",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/tar-iterator.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "",
