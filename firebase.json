{
  "hosting": {

        "appAssociation": "AUTO",
        "rewrites": [
          {
            "source": "/fbase/**",
            "dynamicLinks": true
          },
          {
            "source": "/en/GitHub/**",
            "dynamicLinks": true
          },
          {
            "source": "/link/**",
            "dynamicLinks": true
          },
          {
            "source": "/**",
            "dynamicLinks": true
          },
          {
            "source": "/en/**",
            "dynamicLinks": true
          }
        ],

    "public": "public-fb",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
